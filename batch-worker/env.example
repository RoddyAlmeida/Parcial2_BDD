# ============================================
# CONFIGURACIÓN SUPABASE (Usuario Batch)
# ============================================
# Opción 1: URL completa de conexión (RECOMENDADO)
# Usa el usuario_batch creado en los scripts SQL
SUPABASE_DB_URL_BATCH=postgresql://usuario_batch:[YOUR-PASSWORD]@aws-0-us-west-2.pooler.supabase.com:5432/postgres

# Opción 2: Componentes individuales (alternativa)
# SUPABASE_DB_USER_BATCH=usuario_batch
# SUPABASE_DB_PASSWORD_BATCH=your_batch_password_here
# SUPABASE_DB_HOST=aws-0-us-west-2.pooler.supabase.com
# SUPABASE_DB_PORT=5432
# SUPABASE_DB_NAME=postgres

# ============================================
# CONFIGURACIÓN REDIS (UPSTASH)
# ============================================
UPSTASH_REDIS_REST_URL=https://splendid-civet-13398.upstash.io
UPSTASH_REDIS_REST_TOKEN=your_upstash_token_here

# ============================================
# CONFIGURACIÓN REDIS (LOCAL - OPCIONAL)
# ============================================
# Si no usas Upstash, puedes usar Redis local:
# REDIS_HOST=localhost
# REDIS_PORT=6379
# REDIS_PASSWORD=

# ============================================
# CONFIGURACIÓN BATCH WORKER
# ============================================
COLA_PRINCIPAL=cola:batch:procesar
COLA_PROCESADAS=cola:batch:procesadas
COLA_FALLIDAS=cola:batch:fallidas
TIMEOUT_BLPOP=30

